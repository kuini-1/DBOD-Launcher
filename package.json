{
  "name": "DBOD-Launcher",
  "version": "1.0.5",
  "description": "Dragon Ball Online Launcher with Auto-Update",
  "main": "main.js",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "adm-zip": "^0.5.10",
    "axios": "^0.27.2",
    "fs-extra": "^10.1.0",
    "electron-updater": "^4.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.2.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.5.1",
    "electron": "^12.0.0",
    "electron-builder": "^22.14.13",
    "html-webpack-plugin": "^4.5.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^6.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "wait-on": "^6.0.1",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "webpack serve --mode development",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:portable": "npm run build && electron-builder --win portable",
    "publish:github": "npm run build && electron-builder --win portable --publish always",
    "publish:github:win": "npm run build && electron-builder --win --publish always",
    "publish:github:all": "npm run build && electron-builder --publish always",
    "update-version": "node scripts/update-version.js"
  },
  "keywords": [
    "dragon-ball-online",
    "launcher",
    "electron",
    "react"
  ],
  "author": "",
  "license": "ISC"
}
